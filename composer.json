{
    "name": "awtechs/expressphp",
    "description": "ExpressPHP - featherweight PHP framework (ePHP)",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "ePHP\\": "src/"
        }
    },
    "scripts": {
        "serve": "php -S localhost:8000 -t public"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}